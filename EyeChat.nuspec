		<file src="bin/Release/*.dll" target="lib/net47" />
		<file src="bin/Release/*.exe" target="lib/net47" />

		<!-- Inclure des fichiers de configuration et ressources supplémentaires -->
		<file src="Assets/Config/**" target="content/Config" />
